<Config>
	<!--场景主界面 local= 1 时置顶 2 时x轴中间对齐 4 置底 8 右对齐 16 左对齐 32 Y轴对齐-->
	<start x="0" y="300" w="450" h="450"  local="2" backimg="panel_back.png" moveable="true">
		<choice uniquename="basechoice" x="-64" y="180" upname="choice_base_up.png" downname="choice_base_down.png"/>
		<choice uniquename="extchoice" x="64" y="180" upname="choice_ext_up.png" downname="choice_ext_down.png"/>
		<panel uniquename="baseinfo" moveable="false" x="" y="60">
			<editfield uniquename="x" x="-50" y="-10" content="横坐标" width="400" height="32"/>
			<editfield uniquename="y" x="-50" y="-50" content="纵坐标" width="400" height="32"/>
			<editfield uniquename="zorder" x="-50" y="-80" content="层次" width="400" height="32"/>
		</panel>
		<panel uniquename="extinfo" moveable="false" x="" y="60">
			<list uniquename="list" scrollable="true" eachwidth="200" eachheight="178" leftspan="10" upspan="2" x="0" y="0">
				<face viewx="-180" viewy="-220" vieww="250" viewh="300" back="viewback.png"/>
			</list>
			<button uniquename="addbg" x="150" y="-180" w="64" h="32" name="增加帧" downimg="btn_common_down.png"
				upimg="btn_common_up.png" r="255" g="0" b="" desc="" fontsize="24"/>
		</panel>
		<button uniquename="ok" x="-44" y="-200" w="64" h="32" name="确定" downimg="btn_common_down.png"
				upimg="btn_common_up.png" r="255" g="0" b="" desc="" fontsize="24"/>
		<button uniquename="cancel" x="44" y="-200" w="64" h="32" name="取消" downimg="btn_common_down.png"
				upimg="btn_common_up.png" r="255" g="0" b="" desc="" fontsize="24"/>
	</start>
</Config>