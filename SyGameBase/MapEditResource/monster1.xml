<?xml version="1.0" encoding="UTF-8" ?>
<Config>
	<rect x="256" y="200" w="512" h="512"/>
   <conbineactions>
		<actions name="1">
		  <action nexttype="0" needtime="0.6" frametype="times" cartoontype="self">
			<!--先播放一号动作 紧接着播放2号动作-->
			<dir id="0" framesname="" framestype="frames" >
			  <frame name="skill01.png"/>
			  <frame name="skill02.png"/>
			  <frame name="skill03.png"/>
			  <frame name="skill04.png"/>
			</dir>
		  </action>
		</actions>
		<actions name="2">
		  <action nexttype="0" needtime="0.6" frametype="times" cartoontype="self">
			<!--先播放一号动作 紧接着播放2号动作-->
			<dir id="0" framesname="" framestype="frames" >
			  <frame name="skill01.png"/>
			  <frame name="skill02.png"/>
			  <frame name="skill03.png"/>
			  <frame name="skill04.png"/>
			</dir>
		  </action>
		</actions>
   </conbineactions>
   <actionpools>
		<pool id="0" type="conbine"/>
		<pool id="1" type="always"/> <!--2号动作池定义默认行为-->
   </actionpools>
   <actionpriority>
		<priority name="move" value="0"/>
		<priority name="idle" value="1"/>
   </actionpriority>
   <actionnames>
		<action name="move" id="1"/>
		<action name="idle" id="2"/>
	</actionnames>
	<myindexs> <!--我的整个区域-->
		<index x="0" y="0"/>
	</myindexs>
	<leftindexs> <!--左部-->
		<index x="0" y="0"/>
	</leftindexs>
	<rightindexs><!--右部-->
		<index x="0" y="0"/>
	</rightindexs>
	<prepaths name="down">
		<path dir="0">
			<point name="" x="0" y="-1" movestate="downing"/>
		</path>
	</prepaths>
	<prepaths name="left">
		<path dir="0">
			<point name="" x="1" y="0" movestate="lefting"/>
		</path>
	</prepaths>
	<prepaths name="jump">
		<path dir="0">
			<point name="" x="1" y="0" movestate="uping"/>
			<point name="" x="2" y="0" movestate="uping"/>
			<point name="" x="3" y="0" movestate="uping"/>
			<point name="" x="4" y="0" movestate="uping"/>
			<point name="" x="5" y="0" movestate="uping"/>
			<point name="" x="6" y="0" movestate="uping"/>
			<point name="" x="7" y="0" movestate="uping"/>
			<point name="" x="8" y="0" movestate="uping"/>
			<!--point name="" x="9" y="0" movestate="uping"/-->
		</path>
	</prepaths>
 </Config>
